<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
    <option name="FONT_FAMILY" value="Noto Mono for Powerline" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="SHOW_NOTIFICATION_AFTER_OPTIMIZE_IMPORTS_ACTION" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btodo\([a-z]*\)\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssColorPreviewInGutter" value="false" />
  </component>
</application>